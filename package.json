{
  "name": "mintbase-react-components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "URL_OF_YOUR_REPOSITORY"
  },
  "scripts": {
    "build:tsc": "tsc",
    "build": "npm run clean && rollup -c",
    "lint:types": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-inlinesvg": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "6.4.4",
    "@storybook/addon-essentials": "6.4.4",
    "@storybook/addon-links": "6.4.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/node-logger": "6.4.4",
    "@storybook/react": "6.4.4",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.3.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "postcss": "^8.3.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^2.2.15",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "material-icons": "^1.10.4",
    "react-tooltip": "^4.2.21"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
